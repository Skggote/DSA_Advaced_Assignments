        "class ListNode:\n",
        "    def __init__(self, value):\n",
        "        self.value = value\n",
        "        self.next = None\n",
        "\n",
        "def deleteZeroSum(head):\n",
        "    dummy = ListNode(0)\n",
        "    dummy.next = head\n",
        "    running_sum = 0\n",
        "    sum_dict = {0: dummy}\n",
        "\n",
        "    while head:\n",
        "        running_sum += head.value\n",
        "        sum_dict[running_sum] = head\n",
        "        head = head.next\n",
        "\n",
        "    head = dummy\n",
        "    running_sum = 0\n",
        "    while head:\n",
        "        running_sum += head.value\n",
        "        if running_sum in sum_dict and sum_dict[running_sum] != head:\n",
        "            head.next = sum_dict[running_sum].next\n",
        "        head = head.next\n",
        "\n",
        "    return dummy.next\n",
        "\n",
        "def print_LL(head):\n",
        "    current = head\n",
        "    while current:\n",
        "        if current.next:\n",
